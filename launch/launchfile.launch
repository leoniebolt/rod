<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(env HOME)/catkin_ws/src/rod/worlds/rod_empty.world"/>
  </include>

  <group ns="scara">
    <param name="tf_prefix" value="scara_" />
    
    <param name="robot_description" textfile="$(find scara_mit_dh_urdf)/urdf/scara_mit_dh_urdf.urdf" />

    <node name="spawn_scara" pkg="gazebo_ros" type="spawn_model"
          args="-urdf -model scara -param robot_description -x 0 -y 0 -z 0.1"
          output="screen" />

    <include file="$(find scara_moveit)/launch/move_group.launch" />
  </group>


